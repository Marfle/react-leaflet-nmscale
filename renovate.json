{
  "extends": ["config:base", ":preserveSemverRanges"],
  "lockFileMaintenance": { "enabled": true },
  "rangeStrategy": "bump",
  "semanticCommitType": "chore",
  "packageRules": [
    {
      "updateTypes": ["minor", "patch", "lockFileMaintenance"],
      "depTypeList": ["devDependencies", "dependencies"],
      "automerge": true
    }
  ]
}
