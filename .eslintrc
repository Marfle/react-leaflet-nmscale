{
  "parser": "babel-eslint",
  "plugins": [
    "react", "flowtype"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended","plugin:react/recommended", "plugin:flowtype/recommended"]
}
